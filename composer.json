{
	"name": "vanity/vanity",
	"description": "User-friendly documentation tool for PHP projects.",
	"keywords": ["docs", "documentation", "reference", "api"],
	"homepage": "http://vanitydoc.org",
	"license": "MIT",
	"authors": [
		{
			"name": "Ryan Parman",
			"homepage": "http://ryanparman.com"
		},
		{
			"name": "Contributors",
			"homepage": "https://github.com/vanity/vanity/contributors"
		}
	],
	"require": {
		"php": ">=5.3.2",

		"ext-simplexml": "*",
		"ext-dom": "*",
		"ext-json": "*",
		"ext-pcre": "*",

		"andrewsville/php-token-reflection": "1.3.*",
		"aptoma/twig-markdown": "dev-master",
		"dflydev/github-gist-twig-extension": "2.0.x-dev",
		"dflydev/markdown": "1.0.*",
        "guzzle/guzzle": "3.0.*",
		"monolog/monolog": "1.*",
		"vanity/reflection-docblock": "dev-master",
		"skyzyx/examplify": ">=1.1",
		"skyzyx/service-listing": "dev-master",
		"skyzyx/spdx-licenses": "dev-master",
		"symfony/class-loader": "2.*",
		"symfony/console": "2.*",
		"symfony/event-dispatcher": "2.*",
		"symfony/filesystem": "2.*",
		"symfony/finder": "2.*",
		"symfony/process": "2.*",
		"symfony/templating": "2.*",
		"symfony/yaml": "2.*",
		"twig/twig": "1.*",
		"vanity/pandoc-templates": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"Vanity": "src/"
		}
	}
}
