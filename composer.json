{
	"name": "vanity/vanity",
	"description": "User-friendly documentation tool for PHP projects.",
	"keywords": ["docs", "documentation", "reference", "api"],
	"homepage": "http://vanitydoc.org",
	"license": "MIT",
	"authors": [
		{
			"name": "Ryan Parman",
			"homepage": "http://ryanparman.com"
		},
		{
			"name": "Contributors",
			"homepage": "https://github.com/vanity/vanity/contributors"
		}
	],
	"repositories": {
		"phpdoc_parallel": {
			"type": "package",
			"package": {
				"name": "docblox/parallel",
				"version": "1.0.0",
				"source": {
					"url": "git://github.com/phpDocumentor/Parallel.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		}
	},
	"require": {
		"php": ">=5.3.2",

		"ext-simplexml": "*",
		"ext-dom": "*",
		"ext-json": "*",
		"ext-pcre": "*",

		"andrewsville/php-token-reflection": "1.3.*",
		"dflydev/markdown": "1.0.*",
		"docblox/parallel": "1.*",
		"monolog/monolog": "1.1.*",
		"phpdocumentor/reflection-docblock": "dev-master",
		"skyzyx/examplify": ">=1.1",
		"symfony/class-loader": "2.0.*",
		"symfony/console": "2.0.*",
		"symfony/event-dispatcher": "2.0.*",
		"symfony/filesystem": "2.0.*",
		"symfony/finder": "2.0.*",
		"symfony/process": "2.0.*",
		"symfony/templating": "2.0.*",
		"symfony/yaml": "2.0.*"
	},
	"autoload": {
		"psr-0": {
			"Vanity": "src/"
		}
	}
}
